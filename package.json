{
  "name": "klimaoslo-kart",
  "version": "1.0.0",
  "private": true,
  "description": "KlimaOslo Kartplattform - Selvhostet kartlÃ¸sning for Oslo kommune",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:admin": "npm run dev -w apps/admin",
    "dev:widget": "npm run dev -w apps/widget",
    "dev:backend": "npm run dev -w apps/backend",
